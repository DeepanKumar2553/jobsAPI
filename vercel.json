{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/(.*)",
            "dest": "app.js"
        }
    ],
    "functions": {
        "app.js": {
            "maxDuration": 60
        }
    }
}